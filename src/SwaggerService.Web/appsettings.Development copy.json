{ 
  "Serilog": {
    "Using":  ["IAZI.Common.Infrastructure"],   
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information",
        "Microsoft.AspNetCore.Authentication": "Information",
        "System.Net.Http.HttpClient": "Information",
        "Microsoft.Hosting.Lifetime": "Information"   
      }
    },    
    "WriteTo": [
      {       
        "Name": "CustomConsole",
        "Args": {                    
          "theme": "IAZI.Serilog.Sinks.SystemConsole.Themes.ConsoleTheme::None, IAZI.Common.Infrastructure",          
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz}¬env=\"{Environment}\"¬logsource=\"{MachineName}\"¬loglevel=\"{Level}\"¬requestip=\"{RequestIp:l}\"¬requestid=\"{RequestId:l}\"¬operationid=\"{OperationId:l}\"¬parentid=\"{ParentId:l}\"¬requestpath=\"{RequestPath:l}\"¬correlationid=\"{Correlationid:l}\"¬userid=\"{UserId:l}\"¬username=\"{UserName:l}\"¬context=\"{SourceContext}\"‖{Message:lj} {Exception:l}‖{NewLine}"
        }
      }
    ],
    "Enrich": [ "FromLogContext","WithMachineName","WithEnvironmentLogging","WithOperationId" ]
  },  
  "ServiceConfiguration": {
    "Environment": "dev",
    "BasePath": "/api/template",  
    "Data": {
      "ConnectionStrings": {
        "DefaultConnection": "Server=192.168.9.91,1433;Database=TEMPLATE_DEV;User ID=dbs_TEMPLATE;Password=xxx;ConnectRetryCount=5;"
      }
    },    
    "Custom": {
      "CustomParameter1": "testdev"
    }
  }
}