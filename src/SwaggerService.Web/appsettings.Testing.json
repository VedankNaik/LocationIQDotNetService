{
  "AllowedHosts": "iazi.ch;*.iazi.ch;localhost",   
  "Serilog": {
    "Using":  ["IAZI.Common.Infrastructure"],   
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information",
        "Microsoft.AspNetCore.Authentication": "Information",
        "System.Net.Http.HttpClient": "Information",
        "Microsoft.Hosting.Lifetime": "Information"   
      }
    },    
    "WriteTo": [
      {       
        "Name": "CustomConsole",
        "Args": {                    
          "theme": "IAZI.Serilog.Sinks.SystemConsole.Themes.ConsoleTheme::None, IAZI.Common.Infrastructure",          
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz}¬env=\"{Environment}\"¬logsource=\"{MachineName}\"¬loglevel=\"{Level}\"¬requestip=\"{RequestIp:l}\"¬requestid=\"{RequestId:l}\"¬operationid=\"{OperationId:l}\"¬parentid=\"{ParentId:l}\"¬requestpath=\"{RequestPath:l}\"¬correlationid=\"{Correlationid:l}\"¬userid=\"{UserId:l}\"¬username=\"{UserName:l}\"¬context=\"{SourceContext}\"‖{Message:lj} {Exception:l}‖{NewLine}"
        }
      }
    ],
    "Enrich": [ "FromLogContext","WithMachineName","WithEnvironmentLogging","WithOperationId" ]
  },  
  "ServiceConfiguration": {
    "Environment": "dev",
    "BasePath": "/api/template",  
    "Data": {
      "ConnectionStrings": {
        "DefaultConnection": "Server=database;Database=TEMPLATE_DEV;User ID=dbs_Template;Password=xxx;ConnectRetryCount=5;"
      }
    },
    "Security": {
      "CorsOrigins": "https://dev.iazi.ch,https://devmy2.iazi.ch,https://devweb.iazi.ch,https://devmy.iazi.ch,https://devapp.iazi.ch",
      "ApiName": "SwaggerService",
      "ApiSecret": "secret",
      "AppName":"template",
      "ServerAuthMode": "Both",
      "TokenIssuerV1": "https://iazi.eu.auth0.com/",
      "TokenIssuerV2": "https://www.iazi.ch/dev",
      "LegacyAudienceClientId": "iJbqXTj9oETMZZWW7q6cRCygNQ4VC0oU",
      "LegacyClientSecret": "Fw5E0kHY6O2XpQD4dkbXRuWctHNYYTkmwUdIDxSI9soD4X9yZOzm6Hgt8iXCRrEp",
      "CSPExcludedEndpoints": [
        {
          "ComparisonMode": "StartsWith",
          "Name": "/swagger",
          "CustomCSP": "default-src 'self'; script-src 'self' 'unsafe-inline'; style-src 'self' 'unsafe-inline'; img-src 'self' data:; connect-src 'self' https://devservices.iazi.ch/api/auth/v2/login https://intservices.iazi.ch/api/auth/v2/login https://testservices.iazi.ch/api/auth/v2/login https://services.iazi.ch/api/auth/v2/login; object-src 'none'; frame-ancestors 'self' https://*.iazi.ch;",
          "CustomCSPLocalDev": "default-src 'self'; script-src 'self' 'unsafe-inline'; style-src 'self' 'unsafe-inline'; img-src 'self' data:; connect-src 'self' https://devservices.iazi.ch/api/auth/v2/login https://intservices.iazi.ch/api/auth/v2/login https://testservices.iazi.ch/api/auth/v2/login https://services.iazi.ch/api/auth/v2/login; object-src 'none'; frame-ancestors 'self' https://*.iazi.ch http://localhost:*;"
        }      
      ]
    },
    "Json": {
      "AllowTrailingCommas": "true",
      "IgnoreNullValues": "false",
      "IgnoreReadOnlyProperties": "true",
      "PropertyNameCaseInsensitive": "false",
      "UsePropertyNamingPolicyCamelCase": "false",
      "WriteIndented": "true",      
      "UseJsonStringEnumConverter": "false",
      "UseRelaxedEncoding": "false"
    },
    "Interfaces": {
      "Service.Auth.IdentityServer": {
        "BaseUrl": "https://dev.iazi.ch/api/auth",
        "Name": "IAZI.Service.Auth.IdentityServer"
      },
      "IAZI.Swagger": {
        "Name": "IAZI.Swagger"
      }
    },
    "Custom": {
      "CustomParameter1": "test"
    }
  }  
}
